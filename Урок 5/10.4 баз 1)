#include <iostream>
using namespace std;
int main()
{
int a, b, x, y;
cout << "Enter a: ";
cin >> a;
cout << "Enter b: ";
cin >> b;
if (a < 3*b) {
x = a;
} else {
x = 3*b;
}
if (2*a-b < 26) {
y = 2*a-b;
} else {
y = 26;
}
int z;
if (x < y) {
z = x;
} else {
z = y;
}
// выводим результат
cout << "z = " << z << endl;
return 0;
}
