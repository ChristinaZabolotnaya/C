#include <iostream>
#include <cmath>
using namespace std;

int main() {
    
    const double g = 9.8;
    double V, T, a;
    
    cout << "Введите начальную скорость V (м/с): ";
    cin >> V;
    
    cout << "Введите время полета Т (с): ";
    cin >> T;
    
    a = asin((g*T)/(2*V));
    
    cout << "Угол a, под которым тело брошено: " << a << endl;
    
    return 0;
}
