#include <iostream>
#include <cmath>
using namespace std;

int main() {
    
    int n, d, e, f, m;
    
    cout << "Введите трехзначное число: ";
    cin >> n;
    
    d = n / 100;
    e = (n / 10) % 10;
    f = n % 10;
    m = f * 100 + e * 10 + d;
    
    cout << "Число, прочитанное справа налево: " << m << endl;
    
    return 0;
}
