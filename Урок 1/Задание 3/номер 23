#include <iostream>
#include <cmath>
using namespace std;

int main() {
    
    int n, minutes;
    
    cout << "Введите количество секунд: ";
    cin >> n;
    
    minutes = (n / 60) % 60;
    
    cout << "Количество полных минут с начала последнего часа: " << minutes << endl;
    
    return 0;
}
