#include <iostream>
#include <cmath>
using namespace std;

int main() {
    
    int n, d, e, f, m;
    
    cout << "Введите трехзначное число: ";
    cin >> n;
    
    d = n / 100;
    e = (n / 10) % 10;
    f = n % 10;
    m = d * 100 + f * 10 + e;
    
    cout << "Число после перестановки цифр: " << m << endl;
    
    return 0;
}
