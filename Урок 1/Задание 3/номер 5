#include <iostream>
#include <cmath>
using namespace std;

int main() {
    
    int A, B, n;
    
    cout << "Введите длину отрезка A: ";
    cin >> A;
    
    cout << "Введите длину отрезков B: ";
    cin >> B;
    
    n = A % B;
    
    cout << "Длина незанятой части отрезка A: " << n << endl;
    
    return 0;
}
