#include <iostream>
#include <cmath>
using namespace std;

int main() {
    
    int n;
    
    cout << "Введите трехзначное число: ";
    cin >> n;
    
    int d = n % 10;
    int e = (n / 10) % 10;
    
    cout << "Последняя цифра: " << d << endl;
    cout << "Средняя цифра: " << e << endl;
    
    return 0;
}
