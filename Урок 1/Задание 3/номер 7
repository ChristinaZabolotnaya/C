#include <iostream>
#include <cmath>
using namespace std;

int main() {
    
    int n, d, e, s, p;
    
    cout << "Введите двузначное число: ";
    cin >> n;
    
    d = n / 10;
    e = n % 10;
    s = d + e;
    p = d * e;
    
    cout << "Сумма цифр: " << s << endl;
    cout << "Произведение цифр: " << p << endl;
    
    return 0;
}
