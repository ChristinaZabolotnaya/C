#include <iostream>
#include <cmath>
using namespace std;

int main() {
    
    int k, dayOfWeek;
    
    cout << "Введите номер дня в году (от 1 до 365): ";
    cin >> k;
    
    dayOfWeek = (k + 3) % 7; // +3, так как 1 января - четверг
    
    cout << "Номер дня недели: " << dayOfWeek << endl;
    
    return 0;
}
