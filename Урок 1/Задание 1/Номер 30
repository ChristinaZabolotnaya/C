#include <iostream>
#include <cmath>
using namespace std;

int main() {
   
    double x, y, z, q;
   
    cout << "Введите значение x: ";
    cin >> x;
   
    cout << "Введите значение y: ";
    cin >> y;
   
    z = (x + ((2 + y) / (x * x))) / (y + (1 / sqrt(x * x + 10)));
    q = 2.8 * sin(x) + abs(y);
   
    cout << "z = " << z << endl;
    cout << "q = " << q << endl;
   
    return 0;
}
