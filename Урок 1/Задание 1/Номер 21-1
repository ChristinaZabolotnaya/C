#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double x, y, z;
    
    cout << "Введите значение x: ";
    cin >> x;
    
    cout << "Введите значение y: ";
    cin >> y;
    
    z = pow(x,3) - 2.5*x*y + 1.78*pow(x,2) - 12.5*y + 1;
    
    cout << "z = " << z << endl;
    
    return 0;
}
