#include <iostream>
#include <cmath>
using namespace std;

int main() {
    
    double a, b, c;
    
    cout << "Введите значение a: ";
    cin >> a;
    
    cout << "Введите значение b: ";
    cin >> b;
    
    c = sqrt(a * a + b * b);
    
    cout << "Hypotenuse = " << c << endl;
    
    return 0;
}
