#include <iostream>
#include <cmath>

using namespace std;

int main() {
    
    double A, B, C, AC, BC, product;
    
    cout << "Введите координаты трех точек на числовой оси: ";
    cin >> A >> B >> C;
    
    AC = abs(C - A);
    BC = abs(C - B);
    product = AC * BC;
    
    cout << "Произведение длин отрезков АС и ВС: " << product << endl;
    
    return 0;
}
