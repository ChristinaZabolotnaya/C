#include <iostream>
#include <cmath>

using namespace std;

int main() {
    
    double x1, x2, distance;
    
    cout << "Введите координаты двух точек на числовой оси: ";
    cin >> x1 >> x2;
    
    distance = abs(x2 - x1);
    
    cout << "Расстояние между точками: " << distance << endl;
    
    return 0;
}
