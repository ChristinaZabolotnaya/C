#include <iostream>
#include <cmath>

using namespace std;

int main() {
    
    double A, B;
    
    cout << "Введите значения переменных А и В: ";
    cin >> A >> B;
    
    double temp = A;
    A = B;
    B = temp;
    
    cout << "Новое значение А: " << A << endl;
    cout << "Новое значение В: " << B << endl;
    
    return 0;
}
