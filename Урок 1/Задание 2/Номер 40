#include <iostream>
#include <cmath>
using namespace std;

int main() {
    
    double A1, B1, C1, A2, B2, C2;
    
    cout << "Введите коэффициенты системы линейных уравнений:" << endl;
    cout << "A1*x + B1*y = C1" << endl;
    cout << "A1 = ";
    cin >> A1;
    
    cout << "B1 = ";
    cin >> B1;
    
    cout << "C1 = ";
    cin >> C1;
    
    cout << "A2*x + B2*y = C2" << endl;
    cout << "A2 = ";
    cin >> A2;
    
    cout << "B2 = ";
    cin >> B2;
    
    cout << "C2 = ";
    cin >> C2;
    
    double D = A1*B2 - A2*B1;
    double x = (C1*B2 - C2*B1)/D;
    double y = (A1*C2 - A2*C1)/D;
    
    cout << "Решение системы линейных уравнений:" << endl;
    cout << "x = " << x << endl;
    cout << "y = " << y << endl;
    
    return 0;
}
