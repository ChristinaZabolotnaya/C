#include <iostream>
using namespace std;

int main() {
    
    double a, b, x;
    
    cout << "Введите коэффициент А: ";
    cin >> a;
    
    cout << "Введите коэффициент В: ";
    cin >> b;
    
    x = -b / a; 
    
    cout << "Решение уравнения " << a << "*x + " << b << " = 0: " << x << endl;
    
    return 0;
}
