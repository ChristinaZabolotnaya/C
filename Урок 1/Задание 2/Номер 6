#include <iostream>
#include <cmath>

using namespace std;

int main() {
    
    double a, b, c, V, S;
    
    cout << "Введите длины ребер прямоугольного параллелепипеда: ";
    cin >> a >> b >> c;
    
    V = a * b * c;
    S = 2 * (a * b + b * c + a * c);
    
    cout << "Объем прямоугольного параллелепипеда: " << V << endl;
    cout << "Площадь поверхности прямоугольного параллелепипеда: " << S << endl;
    
    return 0;
}
