#include <iostream>
#include <cmath>

using namespace std;

int main() {
    
    int x;
    cin >> x;
    
    if (x >= 1 && x <= 9) {
        cout << "однозначное ";
    } 
    
    else {
        if (x >= 10 && x <= 99) {
            cout << "двузначное ";
        } 
        else {
            cout << "трехзначное ";
        }
    }
    
    if (x % 2 == 0) {
        cout << "четное число";
    }   
    else {
        cout << "нечетное число";
    }
    
    return 0;
}
