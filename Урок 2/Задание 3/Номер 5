#include <iostream>
#include <cmath>

using namespace std;

int main() {
    
    int x, y;
    cin >> x;
    
    double a, b;
    
    switch (x) {
        case 1:
            cin >> a >> b;
            cout << a + b;
            break;
        case 2:
            cin >> a >> b;
            cout << a - b;
            break;
        case 3:
            cin >> a >> b;
            cout << a * b;
            break;
        case 4:
            cin >> a >> b;
            cout << a / b;
            break;
        default:
            cout << "ошибка";
    }
    
    return 0;
}
