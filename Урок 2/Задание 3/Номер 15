#include <iostream>
#include <cmath>

using namespace std;

int main() {

    int n, m;
    cin >> n >> m;
    
    switch(n) {
        case 6:
            cout << "шесть ";
            break;
        case 7:
            cout << "семь ";
            break;
        case 8:
            cout << "восемь ";
            break;
        case 9:
            cout << "девять ";
            break;
        case 10:
            cout << "десять ";
            break;
        case 11:
            cout << "валет ";
            break;
        case 12:
            cout << "королева ";
            break;
        case 13:
            cout << "король ";
            break;
        case 14:
            cout << "туз ";
            break;
        default:
            cout << "Некорректно" << endl;
            return 0;
    }
    
    switch(m) {
        case 1:
            cout << "пики" << endl;
            break;
        case 2:
            cout << "трефы" << endl;
            break;
        case 3:
            cout << "бубны" << endl;
            break;
        case 4:
            cout << "червы" << endl;
            break;
        default:
            cout << "Некорректно" << endl;
    }
    
    return 0;
}
