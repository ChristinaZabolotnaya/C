#include <iostream>

using namespace std;

int main() {
    int a, b, c;
    cin >> a >> b >> c;
    
    // Треугольник со сторонами а, b, c является прямоугольным
    
    if ( a*a + b*b == c*c || a*a + c*c == b*b || b*b + c*c == a*a ) { 
        cout << "True";
    } 
    
    else {
        cout << "False";
    }
    
    return 0;
}
