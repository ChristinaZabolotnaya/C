#include <iostream>

using namespace std;

int main() {
   
    int x, y;
    cin >> x >> y;
    
    if ((x > 0 && y > 0) || (x < 0 && y < 0)) { // Точка с координатами (x, y) лежит в первой или третьей координатной четверти
        cout << "True";
    } 
    
    else {
        cout << "False";
    }
    
    return 0;
}
