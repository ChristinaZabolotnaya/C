#include <iostream>

using namespace std;

int main() {
   
    int a, b, c;
    cin >> a >> b >> c;
    
    // Ровно одно из чисел А, В, С положительное
    
    if ((a > 0 && b <= 0 && c <= 0) || (a <= 0 && b > 0 && c <= 0) || (a <= 0 && b <= 0 && c > 0)) {  
        cout << "True";
    } 
    
    else {
        cout << "False";
    }
    
    return 0;
}
