#include <iostream>
using namespace std;
int main() {
    double a, sum = 0;
    int n = 1;
    cout << "Введите значение a: ";
    cin >> a;
    while (sum <= a) {
        sum += 1.0/n;
        n++;
    }
    cout << "Результат: ";
    for (int i = 1; i < n; i++) {
        cout << i << " ";
    }
    cout << endl;
    return 0;
}
